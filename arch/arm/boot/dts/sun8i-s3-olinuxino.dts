/dts-v1/;
#include "sun8i-v3.dtsi"
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "S3-OLinuXino";
	/* FIXME with agreed lines */
	compatible = "olimex,s3-olinuxino", "allwinner,sun8i-s3",
		     "allwinner,sun8i-v3";

	aliases {
		serial0 = &uart1;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	leds {
		compatible = "gpio-leds";

		led-red {
			label = "s3-olinuxino:led:red";
			gpios = <&pio 1 5 GPIO_ACTIVE_HIGH>; /* PB5 */
		};
	};

/*
	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		csi0_memory: pool {
			compatible = "shared-dma-pool";
			size = <0x4000000>;
			no-align;
			no-map;
		};
	};
*/

	imx219_supply_parent: imx219-supply-parent {
		compatible = "regulator-fixed";
		regulator-name = "imx219-supply-parent";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&reg_dcdc3>;
		gpio = <&pio 4 24 GPIO_ACTIVE_HIGH>; /* PE24 */
		enable-active-high;
	};

	imx219_supply_vdig: imx219-supply-vdig {
		compatible = "regulator-fixed";
		regulator-name = "imx219-supply-vdig";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&imx219_supply_parent>;
	};

	imx219_supply_vana: imx219-supply-vana {
		compatible = "regulator-fixed";
		regulator-name = "imx219-supply-vana";
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
		vin-supply = <&imx219_supply_parent>;
	};

	imx219_supply_vddl: imx219-supply-vddl {
		compatible = "regulator-fixed";
		regulator-name = "imx219-supply-vddl";
		regulator-min-microvolt = <1200000>;
		regulator-max-microvolt = <1200000>;
		vin-supply = <&imx219_supply_parent>;
	};

	imx219_clock: clock {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <24000000>;
	};
};


